<h2>Dashboard</h2>
<h2>NYSE Stocks</h2>

<h3>Welcome to your Dashboard <%= currentUser.name %></h3>
<% console.log(currentUser) %>

<form method="POST" action="/dashboard/<%= stocks.name %>?_method=DELETE">
    <input hidden type="text" name="name" value="<%= stocks.name %>">
    <input hidden type="text" name="symbol" value="<%= stocks.symbol %>">
    <button type="submit">Delete from Tracker</button>
</form>
