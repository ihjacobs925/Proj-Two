<h2>NYSE Stocks</h2>
    <form method="GET" action="/stocks">
        <input type="text" placeholder="Comp name or Symbol" name="searchTerm">
        <input type="submit" placeholder="Search">
    </form>

<div class="container">
  <% stocks.forEach(function(stocks) { %>
    <div>
      <h2><%= stocks.symbol %> <%= stocks.name %></h2>
      <form class="stock_box" method="POST" action="/dashboard">
        <input hidden type="text" name="name" value="<%= stocks.name %>">
        <input hidden type="text" name="symbol" value="<%= stocks.symbol %>">
        <button type="submit">Add Stock to Tracker</button>
      </form>
    </div>
  <% }); %>
</div>